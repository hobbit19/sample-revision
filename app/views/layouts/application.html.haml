!!!html
%head
  %title 
    = title
  = stylesheet_link_tag :application
  = javascript_include_tag :application
  = csrf_meta_tags
  
%body
  = render "layouts/header"
  .container
    = render_breadcrumb    
    = notice_message
    .content.round
      = yield
    = render "layouts/footer"
    = debug(params) if Rails.env.development?
